@model SIGIT.Models.Aplicacione

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar Aplicación</h1>

<h3 class="text-danger">¿Esta seguro que quiere eliminar este registro?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NombreAplicacion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreAplicacion)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="AplicacionId" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a class="btn btn-outline-secondary" asp-action="Index">Volver</a>
    </form>
</div>
