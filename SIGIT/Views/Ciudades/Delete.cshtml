@model SIGIT.Models.Ciudade

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar Ciudad</h1>

<h3 class="text-danger">¿Esta seguro que quiere eliminar esta ciudad?</h3>

<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NombreCiudad)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NombreCiudad)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="CiudadId" />
        <input type="submit" value="Eliminar" class="btn btn-outline-danger" /> |
        <a class="btn btn-outline-secondary" asp-action="Index">Volver</a>
    </form>
</div>
